{
    "name": "demidovich/php-metric",
    "description": "Prometheus metrics wrapper for PHP applications.",
    "keywords": ["php", "metric"],
    "license": "Apache-2.0",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "endclothing/prometheus_client_php": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Metric\\": "src/Metric"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
