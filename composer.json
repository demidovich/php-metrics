{
    "name": "demidovich/php-metric",
    "license": "MIT",
    "type": "library",
    "description": "Prometheus metrics wrapper for the framework agnostic PHP applications.",
    "keywords": [
        "php",
        "metric",
        "prometheus"
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Metric\\": "src/Metric"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "endclothing/prometheus_client_php": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
