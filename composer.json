{
    "name": "demidovich/php-metrics",
    "license": "MIT",
    "type": "library",
    "description": "Framework agnostic simple library for monitoring PHP application in the Prometheus.",
    "keywords": [
        "php",
        "metric",
        "prometheus"
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Metrics\\": "src/Metrics"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=8.1",
        "psr/log": "^2.0 || ^3.0",
        "promphp/prometheus_client_php": ">=2.6.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
